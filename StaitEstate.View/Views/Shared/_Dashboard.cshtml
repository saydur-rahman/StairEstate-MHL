    <script>

        var scrollHandle = 0;
        var scrollStep = 5;
        var parent = $('.tabs-all');
        //Start the scrolling process
        //            $(".panner").on("mouseenter", function () {
        ////                var data = $(this).data('scrollModifier'),
        ////                    direction = parseInt(data, 10);
        //
        //
        //            });

        //Kill the scrolling
        //            $(".panner").on("mouseleave", function () {
        //                stopScrolling();
        //                $(this).removeClass('active');
        //            });
        $('#lefticon').on('mouseenter', function () {
            $(this).addClass('active');
            var direction = parseInt(-1);
            startScrolling(direction, scrollStep);
        });
        $('#righticon').on('click', function () {
            $(this).addClass('active');
            var direction = parseInt(1);
            startScrolling(direction, scrollStep);
        });
        //add new page on dashboard on menue click
        $(".menu-element").on('click', function () {
            var source = $(this).attr("source");
            var title = $(this).attr("title");
            var addFlag = true;
            inActiveAllTab();
            $('.each-tab').each(function () {
                if ($(this).attr('source') == source) {
                    activeTabBySource(source);
                    addFlag = false;
                    activeTheFrame(source);
                    return;
                }
            });
            if (addFlag) {
                $('.tabs-all').append("<div class='btn bg-pink waves-effect each-tab tab-active' source='" + source + "'><span>" + title + "</span><i class='material-icons cross-icon'>&#xE5C9;</i></div>");

                //inactiveAllFrames();
                $('.body-iframe-holder').append("<iframe src='" + source + "' class='activeFrame body-iframe'></iframe>");
            }
        });
        //end of add new page on dashboard on menue click
        // tab selection
        $(document).on('click', ".each-tab", function () {
            inActiveAllTab();
            if ($(this).hasClass('tab-active')) {
                activeTheFrame($(this).attr("source"));
            }
            else {
                $(this).addClass('tab-active');
                activeTheFrame($(this).attr("source"));
            }
        });
        //tab selection ends
        //closing the tab
        $(document).on('click', ".cross-icon", function (e) {
            e.stopPropagation();
            var source = $(this).parent().attr("source");

            removeTab(source);

        });
        function removeTab(source) {
            $('.body-iframe').each(function () {
                if ($(this).attr("src") == source) {
                    $(this).remove();
                }
            });
            $('.each-tab').each(function () {
                if ($(this).attr("source") == source) {
                    $(this).remove();
                }
            });
        }
        //end of closing tab
        function startScrolling(modifier, step) {
            if (scrollHandle === 0) {
                scrollHandle = setInterval(function () {
                    var newOffset = parent.scrollLeft() + (scrollStep * modifier);

                    parent.scrollLeft(newOffset);
                }, 10);
            }
        }

        function stopScrolling() {
            clearInterval(scrollHandle);
            scrollHandle = 0;
        }
        //if one element is existing inside another element and returns how many elements are there
        function DOMcountElementsInside(parentString, childString) {
            var total = $(parentString).find(childString).count;
            return total;
        }
        function DOMcountAttrElementsInside(parentString, childString) {
            var total = $(parentString).find(childString).count;
            return total;
        }
        function inactiveAllFrames() {
            $('.activeFrame').addClass('inactiveFrame');
            $('.activeFrame').removeClass('activeFrame');
        }
        function activeTheFrame(source) {
            $('.activeFrame').addClass('inactiveFrame');
            $('.activeFrame').removeClass('activeFrame');
            //          $('.body-iframe-holder').find("[src='"+source+"']").addClass("activeFrame");
            $('.body-iframe').each(function () {
                //alert($(this).attr("src")+"----"+source);
                if ($(this).attr("src") == source) {
                    $(this).removeClass("inactiveFrame");
                    $(this).addClass("activeFrame");
                }
            });

        }
        function checkSimilarity(a, b) {
            for (var i = 0, len = Math.max(a.length, b.length); i < len; i++)
                if (a.charAt(i) != b.charAt(i))
                    return Math.round(i / len * 100);
        }
        function activeTabBySource(source) {
            $('.each-tab').each(function () {
                if ($(this).attr("source") == source) {
                    $(this).addClass("tab-active");
                }
            });
        }
        function inActiveAllTab() {
            $('.each-tab').removeClass("tab-active");
        }
        function activeTheTab(elem) {
            elem.addClass("tab-active");
        }
    </script>
</body>
</html>


